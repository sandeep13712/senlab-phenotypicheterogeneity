<CompuCell3D Revision="20140610" Version="3.7.2">
   
   <Potts>
      <!-- Basic properties of CPM (GGH) algorithm -->
      <Dimensions x="500" y="500" z="1"/>
      <Steps>2010</Steps>
      <Temperature>0.01</Temperature>
      <NeighborOrder>1</NeighborOrder>
      <LatticeType>Square</LatticeType>
   </Potts>
   
   <Plugin Name="CellType">
      <!-- Listing all cell types in the simulation -->
      <CellType TypeId="0" TypeName="Medium"/>
      <CellType TypeId="1" TypeName="Cancer"/>
      <CellType TypeId="2" TypeName="Matrix" Freeze=""/>
   </Plugin>      

   <Plugin Name="Surface">
<!--    <SurfaceEnergyParameters CellType="Cancer" TargetSurface="35" LambdaSurface="0.5"/> -->
<!--    <SurfaceEnergyParameters CellType="Epithelial" TargetSurface="35" LambdaSurface="0.5"/> -->
   </Plugin>
   
   <Plugin Name="Volume">
<!--    <VolumeEnergyParameters CellType="Cancer" TargetVolume="100" LambdaVolume="1.0"/> -->
<!--    <VolumeEnergyParameters CellType="Epithelial" TargetVolume="100" LambdaVolume="1.0"/> -->
   </Plugin>    
  
   <Plugin Name="CenterOfMass"/>
   <Plugin Name="PixelTracker"/>
   
    <Plugin Name="BoundaryPixelTracker">
        <NeighborOrder>1</NeighborOrder>
    </Plugin>

 <Plugin Name="Contact">
<!--       Specification of adhesion energies -->
      <Energy Type1="Cancer" Type2="Cancer">40.0</Energy>
      <Energy Type1="Medium" Type2="Cancer">32.0</Energy>
      <Energy Type1="Cancer" Type2="Matrix">16.0</Energy>
      <Energy Type1="Matrix" Type2="Matrix">5.0</Energy>
      <Energy Type1="Medium" Type2="Matrix">35.0</Energy>
      <Energy Type1="Medium" Type2="Medium">35.0</Energy> 
      
      
<!--       <Energy Type1="Cancer" Type2="Cancer">40.0</Energy> -->
<!--       <Energy Type1="Matrix" Type2="Cancer">16.0</Energy> -->
<!--       <Energy Type1="Medium" Type2="Cancer">16.0</Energy> -->
      
<!--       <Energy Type1="Epithelial" Type2="Cancer">24.0</Energy> -->
      
   </Plugin>
   
<Plugin Name="ConnectivityGlobal">
<Penalty Type="Cancer">1000000</Penalty>
</Plugin>
<!-- PixelBased  -->
<Plugin Name="ExternalPotential">
<Algorithm>PixelBased</Algorithm>
   </Plugin>
   
<Steppable Type="FlexibleDiffusionSolverFE">
      
      <!-- Specification of PDE solvers -->
      <DiffusionField Name="EGF">
         <DiffusionData>
            <FieldName>EGF</FieldName>
            <DiffusionConstant>0.0</DiffusionConstant>
            <DecayConstant>1e-05</DecayConstant>
            <!-- Additional options are: -->
            <!-- <DoNotDiffuseTo>LIST YOUR CELL TYPES HERE</DoNotDiffuseTo> -->
            <!-- <DoNotDecayIn>LIST YOUR CELL TYPES HERE</DoNotDecayIn> -->
            <InitialConcentrationExpression>x/500</InitialConcentrationExpression>
            <!-- <ConcentrationFileName>INITIAL CONCENTRATION FIELD - typically a file with path Simulation/NAME_OF_THE_FILE.txt</ConcentrationFileName> -->
            <!-- To run solver for large diffusion constants you typically call solver multiple times - ExtraTimesPerMCS to specify additional calls to the solver in each MCS  -->
            <!-- IMPORTANT: make sure not to mix this setting with the PDESolverCaller module! See manual for more information -->
            <!-- <ExtraTimesPerMCS>0</ExtraTimesPerMCS> -->
            <!-- <DeltaX>1.0</DeltaX> -->
            <!-- <DeltaT>1.0</DeltaT> -->
         </DiffusionData>
         <BoundaryConditions>
            <Plane Axis="X">
               <ConstantValue PlanePosition="Min" Value="0.0"/>
               <ConstantValue PlanePosition="Max" Value="100.0"/>
               <!-- Other options are (examples): -->
               <!-- <Periodic/> -->
               <!-- <ConstantDerivative PlanePosition="Min" Value="10.0"/> -->
            </Plane>
            <Plane Axis="Y">
               <ConstantDerivative PlanePosition="Min" Value="0.0"/>
               <ConstantDerivative PlanePosition="Max" Value="0"/>
               <!-- Other options are (examples): -->
               <!-- <Periodic/> -->
               <!-- <ConstantValue PlanePosition="Min" Value="10.0"/> -->
            </Plane>
         </BoundaryConditions>
      </DiffusionField>
   </Steppable> 
<Plugin Name="NeighborTracker"/>
<Plugin Name="Chemotaxis">
      
      <!-- You may repeat ChemicalField element for each chemical field declared in the PDE solvers -->
      <!-- Specification of chemotaxis properties of select cell types. -->
      <ChemicalField Name="EGF" Source="FlexibleDiffusionSolverFE">
         <ChemotaxisByType Type="Cancer" Lambda="5000" />
      </ChemicalField>
   </Plugin>
   
   <Steppable Type="DiffusionSolverFE">
      <DiffusionField Name="MMP">
         <DiffusionData>
            <FieldName>MMP</FieldName>
            <GlobalDiffusionConstant>0.025</GlobalDiffusionConstant> 
            <ExtraTimesPerMCS>36</ExtraTimesPerMCS>
            <GlobalDecayConstant>0.002</GlobalDecayConstant>
            <InitialConcentrationExpression>0</InitialConcentrationExpression>
         </DiffusionData>
         <SecretionData>
            <!-- <Secretion Type="Cancer">0.00</Secretion> -->
            <SecretionOnContact SecreteOnContactWith="Matrix" Type="Cancer">0.05</SecretionOnContact> 
            <!-- <ConstantConcentration Type="Cancer">0.1</ConstantConcentration> -->
         </SecretionData>

         <BoundaryConditions>
            <Plane Axis="X">
               <ConstantValue PlanePosition="Min" Value="0.0"/>
               <ConstantValue PlanePosition="Max" Value="0.0"/>
               <!-- Other options are (examples): -->
               <!-- <Periodic/> -->
               <!-- <ConstantDerivative PlanePosition="Min" Value="10.0"/> -->
            </Plane>
            
            <Plane Axis="Y">:
               <ConstantDerivative PlanePosition="Min" Value="0.0"/>
               <ConstantDerivative PlanePosition="Max" Value="0.0"/>
               <!-- Other options are (examples): -->
               <!-- <Periodic/> -->
               <!-- <ConstantValue PlanePosition="Min" Value="10.0"/> -->
            </Plane>
         </BoundaryConditions>
      </DiffusionField>
         </Steppable>
         
<Steppable Type="PIFInitializer">
<PIFName>Lattice_600Fibrers.piff</PIFName> 
</Steppable>

